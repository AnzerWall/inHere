    "joi": "^9.0.4",
    "lodash": "^4.15.0",
    "moment": "^2.15.1",
    "redis": "^2.6.2",
    "redis-connection-pool": "^1.5.0",
    "socket.io-redis": "^1.0.0",
    "source-map-support": "0.4.0",
    "think-cors": "^1.0.8",
    "thinkjs": "2.2.x",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-core": "6.x.x"
  },
